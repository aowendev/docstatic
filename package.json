{
  "name": "docstatic",
  "version": "0.3.9",
  "description": "A Docusaurus + TinaCMS documentation/static site generator with OpenAPI and GraphQL support.",
  "author": "aowendev <aowen@ieee.org>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aowendev/docstatic.git"
  },
  "keywords": [
    "docusaurus",
    "tinacms",
    "documentation",
    "static site",
    "openapi",
    "graphql"
  ],
  "scripts": {
    "docusaurus": "node --localstorage-file=./localstorage.json ./node_modules/.bin/docusaurus.mjs",
    "dev": "tinacms dev -c \"node --localstorage-file=./localstorage.json ./node_modules/.bin/docusaurus.mjs start\" ",
    "start": "node --localstorage-file=./localstorage.json ./node_modules/.bin/docusaurus.mjs start",
    "generate-files": "node scripts/generate-file-list.js",
    "prebuild": "yarn generate-files",
    "predev": "yarn generate-files",
    "build": "tinacms build && node --localstorage-file=./localstorage.json ./node_modules/.bin/docusaurus.mjs build",
    "build-local": "tinacms build --local --skip-indexing --skip-cloud-checks && node --localstorage-file=./localstorage.json ./node_modules/.bin/docusaurus.mjs build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "gen-api-docs": "docusaurus gen-api-docs",
    "clean-api-docs": "docusaurus clean-api-docs",
    "gen-graphql": "docusaurus docs:generate:graphql",
    "gen-api-docs:version": "docusaurus gen-api-docs:version",
    "clean-api-docs:version": "docusaurus clean-api-docs:version",
    "lint": "biome check apis/ config/ reuse/ scripts/ src/ static/ tina/",
    "lint:fix": "biome check apis/ config/ reuse/ scripts/ src/ static/ tina/ --fix"
  },
  "dependencies": {
    "@algolia/client-search": "^5.43.0",
    "@codemirror/language": "6.0.0",
    "@docusaurus/core": "^3.9.2",
    "@docusaurus/plugin-content-docs": "^3.9.2",
    "@docusaurus/preset-classic": "^3.9.2",
    "@docusaurus/theme-common": "^3.9.2",
    "@docusaurus/theme-mermaid": "^3.9.2",
    "@docusaurus/utils": "^3.9.2",
    "@docusaurus/utils-validation": "^3.9.2",
    "@mdx-js/react": "^3.0.0",
    "@types/react": "^18.3.1",
    "color": "^5.0.2",
    "docusaurus-graphql-plugin": "0.7.0",
    "docusaurus-lunr-search": "^3.6.0",
    "docusaurus-plugin-openapi-docs": "4.5.1",
    "docusaurus-plugin-sass": "^0.2.3",
    "docusaurus-theme-openapi-docs": "4.5.1",
    "fs-extra": "9.0.1",
    "raw-loader": "^4.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-loadable": "^5.5.0",
    "react-markdown": "^10.1.0",
    "rehype-katex": "^7.0.1",
    "remark-breaks": "^4.0.0",
    "remark-math": "^6.0.0",
    "sass": "^1.93.3",
    "scheduler": "^0.27.0",
    "search-insights": "^2.17.3",
    "slate": "^0.118.1",
    "slate-dom": "^0.118.1",
    "sucrase": "^3.35.0",
    "tinacms": "^2.9.1",
    "title": "^4.0.1",
    "typescript": "5.6.3",
    "webpack": "^5.102.1",
    "yup": "0.32.11"
  },
  "resolutions": {
    "graphql": "15.8.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.4",
    "@tinacms/cli": "^1.12.0"
  },
  "engines": {
    "node": ">=20.0"
  }
}
